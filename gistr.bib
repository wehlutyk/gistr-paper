	Date = {2004-02-01},
	Doi = {10.1080/02643290342000122},
	File = {Nickels_Howard_2004_Dissociating Effects of Number of Phonemes, Number of Syllables, and Syllabic.pdf:/home/sl/Zotero/storage/CF5B74W3/Nickels_Howard_2004_Dissociating Effects of Number of Phonemes, Number of Syllables, and Syllabic.pdf:application/pdf;Snapshot:/home/sl/Zotero/storage/KHFMV7R4/02643290342000122.html:text/html},
