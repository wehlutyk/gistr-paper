	Issn = {0011-3204},
	Journaltitle = {Current Anthropology},
	Number = {4},
	Pages = {434--470},
	Shortjournal = {Current Anthropology},
	Shorttitle = {Genes, Culture, and Agriculture},
	Title = {Genes, Culture, and Agriculture: An Example of Human Niche Construction},
