	File = {ScienceDirect Snapshot:/home/sl/Zotero/storage/VI66VFMB/S0010027710001320.html:text/html;Smith_Wonnacott_2010_Eliminating unpredictable variation through iterated learning.pdf:/home/sl/Zotero/storage/ZFD8G2GX/Smith_Wonnacott_2010_Eliminating unpredictable variation through iterated learning.pdf:application/pdf},
