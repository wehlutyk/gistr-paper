	Number = {12},
	Pages = {e0168306},
	Shortjournal = {{PLOS} {ONE}},
	Shorttitle = {Beyond Synchrony},
	Title = {Beyond Synchrony: Joint Action in a Complex Production Task Reveals Beneficial Effects of Decreased Interpersonal Synchrony},
