	File = {Snapshot:/home/sl/Zotero/storage/H3IJV4G7/83E9F516875C340E0A9263B4A7C38F43.html:text/html;Winters et al_2015_Languages adapt to their contextual niche.pdf:/home/sl/Zotero/storage/VC2JN3DK/Winters et al_2015_Languages adapt to their contextual niche.pdf:application/pdf},
	Issn = {1866-9808, 1866-9859},
