	File = {Potter_Lombardi_1998_Syntactic Priming in Immediate Recall of Sentences.pdf:/home/sl/Zotero/storage/ZFT2I73D/Potter_Lombardi_1998_Syntactic Priming in Immediate Recall of Sentences.pdf:application/pdf;ScienceDirect Snapshot:/home/sl/Zotero/storage/AEPTXFXA/S0749596X97925468.html:text/html},
	Issn = {0749-596X},
	Journaltitle = {Journal of Memory and Language},
	Keywords = {read-abstract, sentence recall, syntax regeneration, syntactic priming},
	Number = {3},
	Pages = {265--282},
	Shortjournal = {Journal of Memory and Language},
